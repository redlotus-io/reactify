{
  "name": "react-boilerplate",
  "private": true,
  "version": "2.0.1",
  "module": "src/main.tsx",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "clean": "rm -rf node_modules/ && rm -rf yarn.lock && yarn",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@redlotus/ui": "^0.0.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "tailwindcss-border-gradient-radius": "^3.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@redlotus/tsconfig": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "eslint-config-redlotus-react": "0.1.5",
    "husky": "^8.0.2",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "tailwind-scrollbar": "^2.0.1",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-pwa": "^0.13.3",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,graphql}": "yarn lint:fix"
  }
}
